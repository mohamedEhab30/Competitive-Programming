#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
const ll mod=1e9+7,SZ=10000007;
vector<int>isprime(SZ,1);
vector<int>primes;
void Sieve(ll n)
{
    isprime[0]=isprime[1]=0;
    n = n + 1;
    for(ll i=2; i<=n ;i++)
    {
        if(isprime[i])
        {
            for(ll j=i*i;j<=n;j+=i)
                isprime[j]=0;
            primes.push_back(i);
        }
    }
}
int main()
{
#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
#endif
    vector<bool>isp(1000,1);
    isp[0]=isp
}

